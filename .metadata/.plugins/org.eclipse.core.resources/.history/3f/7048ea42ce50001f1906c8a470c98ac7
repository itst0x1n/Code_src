#include "stm32f4xx_hal.h"
#include "ledblink.h"
#include "cmsis_os.h"


void ledBlink(ledBlink.blinkTime, ledBlink.pin, ledBlink.pinType){
	HAL_GPIO_TogglePin(pinType, pin);
	osDelay(blinkTime);
 }

typedef struct ledBlink{
	uint16_t blinkTime;
	uint16_t pin;
	GPIO_TypeDef* pinType;
}LedBlink;
